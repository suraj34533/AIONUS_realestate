# ========================================
# AIONUS ENVIRONMENT CONFIGURATION
# ========================================
# Copy this file to .env and fill in your API keys
# DO NOT commit .env to version control!

# Supabase Backend
# Get these from: https://supabase.com/dashboard/project/YOUR_PROJECT/settings/api
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Google Gemini AI (For Text, RAG, Voice Agent)
# Get this from: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your-gemini-api-key

# Google Maps API (For Property Maps + Location Search)
# Get this from: https://console.cloud.google.com/apis/credentials
MAPS_API_KEY=your-maps-api-key

# Voice Agent Config (Optional - has defaults)
GEMINI_TTS_MODEL=gemini-2.0-flash
GEMINI_STT_MODEL=gemini-2.0-flash

# RAG Pipeline (Optional - has defaults)
EMBEDDING_MODEL=text-embedding-004

# Storage bucket for brochures (Supabase)
SUPABASE_BUCKET_NAME=brochures

# ========================================
# TELEGRAM BOT CONFIGURATION
# ========================================
# Get your bot token from @BotFather on Telegram
TELEGRAM_BOT_TOKEN=your-telegram-bot-token

# Comma-separated list of Telegram chat IDs allowed to use admin commands
# Get your chat ID by sending a message to @userinfobot on Telegram
TELEGRAM_ADMIN_CHAT_IDS=123456789,987654321

# After setting up, call POST /telegram/set-webhook with:
# { "webhook_url": "https://your-domain.com/telegram/webhook" }
